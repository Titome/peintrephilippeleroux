{% extends "TitomePeintrePhilippeLerouxBundle::layoutAdmin.html.twig" %}

{% block title %}Ajouter une image{{ parent() }}{% endblock %}

{% block corps %}
    <div class="container">
        <h1>Ajouter une image au carrousel</h1>

        <form action="#" method="post" {{ form_enctype(form) }} class="well">
            {{ form_widget(form) }}

            <input type="submit" value="Envoyer l'image" class="btn"/>
        </form>
    </div>
{% endblock %}