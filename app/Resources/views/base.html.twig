<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Peintre Philippe Leroux{% endblock %}</title>
        {% block stylesheets %}
        {% endblock %}
        {% image '@TitomePeintrePhilippeLerouxBundle/Resources/public/img/favicon.ico' %}
            <link rel="shortcut icon" href="{{ asset_url }}" />
        {% endimage %}
    </head>
    <body {% block body_class %}{% endblock %}>
        {% block body %}
            {% block nav %}{% endblock %}
            {% block corps %}{% endblock %}
        {% endblock %}
        {% block javascripts %}
            {% javascripts
                '@TitomePeintrePhilippeLerouxBundle/Resources/public/js/jquery-1.7.2.min.js'
                '@TitomePeintrePhilippeLerouxBundle/Resources/public/js/bootstrap.min.js'
                '@TitomePeintrePhilippeLerouxBundle/Resources/public/zoombox/zoombox.js'
                '@TitomePeintrePhilippeLerouxBundle/Resources/public/js/turn.min.js'
                output='main.js'
            %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
                
            <script>
                $('.carousel').carousel()
            </script>
        {% endblock %}
    </body>
</html>
